# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Api docs (should be generated by rake task)
public/docs/
public/assets/

# Ignore database configuration file
# /config/database.yml

# Ignore Byebug command history file.
.byebug_history

# Uploads
/public/uploads/*

# Mac OS
.DS_Store

# Entity Relational Diagram
erd.pdf

# Ignore donenv files
.env
.env.test
.env.staging
.env.development

# Logs
/log
